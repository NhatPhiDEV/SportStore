@{
    Layout = null;
}

<!DOCTYPE html>

<html class="no-js">

<head>
    <meta charset="utf-8">
    <title>KING OF SPORTSWEAR</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="ProteusThemes">

    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/responsive.css" rel="stylesheet">
    <link href="~/Scripts/rs-plugin/css/settings.css" rel="stylesheet" type="text/css" />

    <link rel="stylesheet" href="~/Scripts/jquery-ui-1.10.3/css/smoothness/jquery-ui-1.10.3.custom.min.css" type="text/css" />

    <link rel="stylesheet" href="~/Scripts/prettyphoto/css/prettyPhoto.css" type="text/css" />

    <link href="~/Content/main.css" rel="stylesheet">

    <script src="~/Scripts/modernizr.custom.56918.js"></script>
</head>

<body class="">
    <div class="master-wrapper">
        <!-- Header -->
        <header id="header">
            <div class="darker-row">
                <div class="container" style="margin-right:20px">
                    <div class="row">
                        <div class="span4">

                        </div>
                        <div class="span8">
                            <div class="topmost-line">
                                @Html.Action("DisplayLogin", "Home")
                                &nbsp;
                                <div class="lang-currency">
                                    <div class="dropdown js-selectable-dropdown">
                                        <a data-toggle="dropdown" class="selected" href="#"><span class="js-change-text"><i class="famfamfam-flag-vn"></i> Việt Nam (VN)</span> <b class="caret"></b></a>
                                        <ul class="dropdown-menu js-possibilities" role="menu" aria-labelledby="dLabel">
                                            <li><a href="#"><i class="famfamfam-flag-vn"></i> Việt Nam (VN)</a></li>
                                            <li><a href="#"><i class="famfamfam-flag-gb"></i> English (EN)</a></li>
                                        </ul>
                                    </div>
                                    <div class="dropdown js-selectable-dropdown">
                                        <a data-toggle="dropdown" class="selected" href="#"><span class="js-change-text">VND</span> <b class="caret"></b></a>
                                        <ul class="dropdown-menu js-possibilities" role="menu" aria-labelledby="dLabel">
                                            <li><a href="#">VND</a></li>
                                            <li><a href="#">USD</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="row">

                    <div class="span7">
                        <a class="brand" href="@Url.Action("Index","Home")">
                            <img src="~/Content/images/logo.png" alt="Webmarket Logo" width="48" height="48">
                            <span class="pacifico">PingoShop</span>
                            <span class="tagline">Sự hài lòng của các bạn là thành công của chúng tôi!</span>
                        </a>
                    </div>

                    <div class="span5">
                        <div class="top-right">
                            <div class="icons">
                                <a href="#"><span class="zocial-facebook"></span></a>
                                <a href="#"><span class="zocial-skype"></span></a>
                                <a href="#"><span class="zocial-twitter"></span></a>
                                <a href="#"><span class="zocial-rss"></span></a>
                                <a href="#"><span class="zocial-wordpress"></span></a>
                                <a href="#"><span class="zocial-android"></span></a>
                                <a href="#"><span class="zocial-html5"></span></a>
                                <a href="#"><span class="zocial-windows"></span></a>
                                <a href="#"><span class="zocial-apple"></span></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!--End Header -->
        <!-- Menu bar -->
        <div class="navbar navbar-static-top" id="stickyNavbar">
            <div class="navbar-inner">
                <div class="container">
                    <div class="row">
                        <div class="span9">
                            <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>

                            <div class="nav-collapse collapse">
                                <ul class="nav" id="mainNavigation">
                                    <li><a href="@Url.Action("Index","Home")">Trang chủ</a></li>
                                    <li><a href="@Url.Action("GetProduct","Product")">Sản Phẩm</a></li>
                                    <li><a href="@Url.Action("GetProduct","Product")">Áo đội tuyển</a></li>
                                    <li><a href="@Url.Action("GetProduct","Product")">Áo câu lạc bộ</a></li>
                                    <li class="dropdown active">
                                        <a href="@Url.Action("Index","Home")" class="dropdown-toggle"> Giao diện <b class="caret"></b> </a>
                                        <ul class="dropdown-menu">
                                            <li class="dropdown active">
                                                <a href="@Url.Action("Index","Home")"><i class="icon-caret-right pull-right visible-desktop"></i>Màu trang web</a>
                                                <ul class="dropdown-menu">
                                                    <li class="active"><a href="@Url.Action("Index","Home")">Mặc định</a></li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </li>
                                    <li><a href="@Url.Action("Contact","Home")">Liên hệ</a></li>
                                </ul>
                                <form class="navbar-form pull-right" action="#" method="get">
                                    <button type="submit"><span class="icon-search"><i class="fas fa-search"></i></span></button>
                                    <input type="text" class="span1" name="search" id="navSearchInput">
                                </form>
                            </div>
                        </div>

                        <!-- Cart (Giỏ hàng) -->
                        <div class="span3">
                            <div class="cart-container" id="cartContainer">
                                @Html.Action("CartPartial", "Cart")
                                <!-- Hiển thị sản phẩm bên trong giỏ hàng -->
                                <div class="open-panel" style="overflow-y:auto;overflow-x:hidden;width:600px;height:527px;">
                                    @Html.Action("Cart", "Cart")
                                    <div class="proceed">
                                        <a href="@Url.Action("DeleteAll", "Cart")" class="btn btn-danger pull-right bold higher">Xóa tất cả&nbsp;<i style="font-size:20px" class="icon-trash"></i></a>
                                        <a href="@Url.Action("Oder", "Cart")" class="btn btn-danger pull-right bold higher">Thanh Toán&nbsp;<i style="font-size:20px" class="icon-shopping-cart"></i></a>
                                    </div>
                                </div>
                                <!-- Kết thúc hiển thị sản phẩm bên trong giỏ hàng -->
                            </div>
                        </div>
                        <!--End Cart (Giỏ hàng) -->
                    </div>
                </div>
            </div>
        </div>
        <!--End Menu bar -->
        @RenderBody()
        <!-- Footer -->
        <footer>
            <div class="foot-light">
                <div class="container">
                    <div class="row">
                        <div class="span4">
                            <h2 class="pacifico">PingoShop &nbsp; <img src="~/Content/images/webmarket.png" alt="Webmarket Cart" class="align-baseline"></h2>
                            <p>Sản phẩm chất lượng, nhân viên tận tâm phục vụ 24/24 sự hài lòng của các bạn là vinh dự của chúng tôi. </p>
                        </div>
                        <div class="span4">
                            <div class="main-titles lined">
                                <h3 class="title">Facebook</h3>
                            </div>
                        </div>
                        <div class="span4">
                            <div class="main-titles lined">
                                <h3 class="title"><span class="light">Đăng kí nhận thông báo từ chúng tôi</span></h3>
                            </div>
                            <p>Làm ơn nhập email để chúng tôi có thể tìm thấy bạn và cung cấp những chương trình khuyến mãi sớm nhất có thể đến bạn.</p>

                            <div id="mc_embed_signup">
                                <form action="http://proteusthemes.us4.list-manage1.com/subscribe/post?u=ea0786485977f5ec8c9283d5c&amp;id=5dad3f35e9" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate form form-inline" target="_blank" novalidate="">
                                    <div class="mc-field-group">
                                        <input type="email" value="" placeholder="Enter your e-mail address" name="EMAIL" class="required email" id="mce-EMAIL">
                                        <input type="submit" value="Send" name="subscribe" id="mc-embedded-subscribe" class="btn btn-primary">
                                    </div>
                                    <div id="mce-responses" class="clear">
                                        <div class="response" id="mce-error-response" style="display:none"></div>
                                        <div class="response" id="mce-success-response" style="display:none"></div>
                                    </div>
                                </form>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

            <div class="foot-dark">
                <div class="container">
                    <div class="row">

                        <div class="span3">
                            <div class="main-titles lined">
                                <h3 class="title"><span class="light">Main</span> Navigation</h3>
                            </div>
                            <ul class="nav bold">
                                <li><a href="#">Home</a></li>
                                <li><a href="#">Pages</a></li>
                                <li><a href="#">About Us</a></li>
                                <li><a href="#">Shortcodes</a></li>
                                <li><a href="#">Gallery</a></li>
                                <li><a href="#">Contact Us</a></li>
                            </ul>
                        </div>

                        <div class="span3">
                            <div class="main-titles lined">
                                <h3 class="title"><span class="light">Second</span> Navigation</h3>
                            </div>
                            <ul class="nav">
                                <li><a href="#">Lorem Ipsum Dolor Sit</a></li>
                                <li><a href="#">Amet Webmarket Signup</a></li>
                                <li><a href="#">Brands</a></li>
                                <li><a href="#">Latest Tweets Sometging</a></li>
                                <li><a href="#">Ipsum Sit Lorem Amet</a></li>
                            </ul>
                        </div>



                        <div class="span3">
                            <div class="main-titles lined">
                                <h3 class="title"><span class="light">Third</span> Navigation</h3>
                            </div>
                            <ul class="nav">
                                <li><a href="#">Lorem Ipsum Dolor Sit</a></li>
